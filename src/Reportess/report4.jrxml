	<queryString>
		<![CDATA[SELECT
     partido.`Gol_Casa` AS partido_Gol_Casa,
     partido.`Gol_Visita` AS partido_Gol_Visita,
     partido.`Fecha_partido` AS partido_Fecha_partido,
     equipo.`Nombre_equipo` AS equipo_Nombre_equipo,
     campeonato.`Nombre_campeonato` AS campeonato_Nombre_campeonato,
     partido.`idPartido` AS partido_idPartido
FROM
     `partido` partido,
     `equipo` equipo,
     `campeonato` campeonato
WHERE
